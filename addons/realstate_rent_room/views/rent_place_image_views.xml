<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="rent_place_image_form" model="ir.ui.view">
            <field name="name">realstate_rent_room.rent_place_image.form</field>
            <field name="model">realstate_rent_room.rent_place_image</field>
            <field name="arch" type="xml">
                <form string="">
                    <header>

                    </header>
                    <sheet string="">
                        <group>
                            <field name="name" invisible="1"/>
                            <field name="image" filename="name"/>
                            <field name="description"/>
                        </group>
                    </sheet>

                </form>
            </field>
        </record>

        <record id="rent_place_image_tree" model="ir.ui.view">
            <field name="name">realstate_rent_room.rent_place_image.tree</field>
            <field name="model">realstate_rent_room.rent_place_image</field>
            <field name="arch" type="xml">
                <tree string="">
                    <field name="name" />
                    <field name="description" />
                </tree>
            </field>
        </record>

        <record id="rent_place_image_search" model="ir.ui.view">
            <field name="name">realstate_rent_room.rent_place_image.search</field>
            <field name="model">realstate_rent_room.rent_place_image</field>
            <field name="arch" type="xml">
                <search string="">

                </search>
            </field>
        </record>

        <record id="rent_place_image_action" model="ir.actions.act_window">
            <field name="name">realstate_rent_room.rent_place_image.action</field>
            <field name="res_model">realstate_rent_room.rent_place_image</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="rent_place_image_search"/>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">

              </p><p>

              </p>
            </field>
        </record>

        <!--<menuitem id="" action="rent_place_image_action"-->
                  <!--parent="" sequence="" />-->
    </data>
</odoo>
